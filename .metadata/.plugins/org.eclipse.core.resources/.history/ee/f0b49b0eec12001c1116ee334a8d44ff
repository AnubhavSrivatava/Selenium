import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Salesforcelocator {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "/Users/anubhavsrivastava/Documents/Anubhav/Selenium/drivers/chromedriver");
		WebDriver driver = new ChromeDriver();
		driver.get("https://login.salesforce.com/");
		driver.findElement(By.xpath("//div[contains(@class,'login'")).sendKeys("Anubhav");
		driver.findElement(By.cssSelector("div[class*='input']")).sendKeys("passsword");
		driver.findElement(By.xpath("//input[@id='Login']"));
		
	}

}
